  <question type="multichoice">
    <name>
      <text>Conversión km a metros - Ambulancia</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p>Una ambulancia recorre 3.5 km hasta el hospital. ¿Cuántos metros recorre?</p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text><![CDATA[<p>Para convertir kilómetros a metros multiplicamos por 1.000: 3.5 × 1.000 = 3.500 metros</p>]]></text>
    </generalfeedback>
    <defaultgrade>1</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
    <idnumber></idnumber>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>abc</answernumbering>
    <showstandardinstruction>0</showstandardinstruction>
    <correctfeedback format="html">
      <text>¡Correcto!</text>
    </correctfeedback>
    <partiallycorrectfeedback format="html">
      <text>Tu respuesta es parcialmente correcta.</text>
    </partiallycorrectfeedback>
    <incorrectfeedback format="html">
      <text>Respuesta incorrecta.</text>
    </incorrectfeedback>
    <answer fraction="100" format="html">
      <text><![CDATA[<p>3.500 metros</p>]]></text>
      <feedback format="html">
        <text>¡Excelente! 3.5 km × 1.000 = 3.500 metros</text>
      </feedback>
